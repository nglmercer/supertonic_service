{
  "name": "supertonic",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "NODE_ENV=development bun run src/server.ts",
    "demo": "bun run demo.ts",
    "client": "bun run examples/client.ts",
    "test": "bun test",
    "test:server": "bun test src/server.test.ts",
    "build": "bash scripts/build.sh current",
    "build:all": "bash scripts/build.sh all",
    "build:x86_64": "bash scripts/build.sh x86_64",
    "build:arm64": "bash scripts/build.sh arm64",
    "build:docker": "bash scripts/build.sh docker",
    "docker": "docker build -t supertonic . && docker run -p 3000:3000 supertonic"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.10.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.1",
    "@huggingface/transformers": "^3.8.1",
    "@libp2p/mdns": "^12.0.11",
    "@libp2p/tcp": "11.0.10",
    "libp2p": "3.1.3",
    "onnxruntime-node": "1.21.0"
  },
  "trustedDependencies": [
    "onnxruntime-node",
    "protobufjs"
  ]
}